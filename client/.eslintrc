{
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "allowImportExportEverywhere": false,
        "codeFrame": false
    },
    "extends": [
        "prettier"
    ],
    "env": {
        "browser": true,
        "jest": true
    },
    "rules": {
        "max-len": [
            "error",
            {
                "code": 200
            }
        ],
        "prefer-promise-reject-errors": [
            "off"
        ],
        "react/jsx-filename-extension": [
            "off"
        ],
        "no-return-assign": [
            "off"
        ]
    },
    "settings": {
        "react": {
            "version": "^17.0.1"
        }
    }
}